<resources>

    <!-- Default rounded corner curve (a Bezier). Must match (the curved path in) rounded.xml.
         Note that while rounded.xml includes the entire path (including the horizontal and vertical
         corner edges), this pulls out just the curve.
     -->
    <string name="config_rounded_mask" translatable="false">M102,0C71.95,0 44.13,15.47 29.8,29.8C15.47,44.13 0,71.95 0,102</string>
    
    <!-- Define UDFPS vendor code for AOD -->
    <integer name="config_udfps_vendor_code">22</integer>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">7</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">8</integer>

    <!-- Max visible system icons -->
    <integer name="config_maxVisibleStatusIconContainer">9</integer>

    <!-- Should we vibrate on an icon animation of the shelf. This should only be active if the
     vibrator is capable of subtle vibrations -->
    <bool name="config_vibrateOnIconAnimation">true</bool>

</resources>
